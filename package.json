{
  "name": "obsidian-anki-image-occlusion",
  "version": "0.4.0",
  "description": "Convert Obsidian markdown files with highlights into Anki image occlusion cards",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "anki",
    "image-occlusion",
    "spaced-repetition"
  ],
  "author": "Eleanor Cross",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.19.10",
    "obsidian": "latest",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.4.449"
  }
}
